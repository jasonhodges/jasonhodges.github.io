{
  "name": "jasonhodges.codes",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "hmr": "ng serve --configuration hmr",
    "build": "ng build",
    "build:publish": "ng build --prod --base-href '/'",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cz": "git add . && git-cz",
    "publish": "npx ngh --branch=master --dir ./dist --no-silent --name=\"Jason Hodges\" --email=\"jasonhodges.codes@gmail.com\"",
    "publish:full": "yarn build:publish && yarn docs && yarn docs:copy && yarn run publish",
    "docs": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
    "docs:copy": "cp -R ./documentation ./dist/docs",
    "dirparse": "node ./config/dir-parse.js",
    "ngsw-config": "ngsw-config dist .removed/ngsw-config.json /",
    "firebase:deploy": "ng deploy"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular-devkit/schematics": "^17.1.2",
    "@angular/animations": "^17.1.2",
    "@angular/cdk": "^17.1.2",
    "@angular/common": "^17.1.2",
    "@angular/compiler": "^17.1.2",
    "@angular/core": "^17.1.2",
    "@angular/forms": "^17.1.2",
    "@angular/http": "^7.2.16",
    "@angular/material": "^17.1.2",
    "@angular/platform-browser": "^17.1.2",
    "@angular/platform-browser-dynamic": "^17.1.2",
    "@angular/router": "^17.1.2",
    "@angular/service-worker": "^17.1.2",
    "ngx-disqus": "^3.0.1`",
    "rxjs": "^7.8.1",
    "tslib": "^2.4.0",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~17.1.2",
    "@angular/cli": "~17.1.2",
    "@angular/compiler-cli": "^17.1.2",
    "@compodoc/compodoc": "^1.1.23",
    "@types/node": "~20.11.16",
    "codelyzer": "~6.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "front-matter": "^4.0.2",
    "fuzzy": "^0.1.3",
    "inquirer": "^9.2.13",
    "inquirer-autocomplete-prompt": "^3.0.1",
    "lodash": "^4.17.21",
    "ngx-markdown": "^17.1.1",
    "prettier": "^3.2.4",
    "typescript": "5.3.3"
  }
}
